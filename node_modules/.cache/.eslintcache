oComponent />\n        <PowerButton />\n        <SocialIcons />\n        <Center>\n          <Grid>\n            {Designs.map((design) => {\n              return <DesignComponent key={design.id} design={design} />;\n            })}\n          </Grid>\n        </Center>\n        <BigTitle text=\"DESIGN\" top=\"5rem\" left=\"5rem\" />\n      </Container>\n    </MainContainer>\n  );\n};\n\nexport default DesignPage;\n","/home/zoney/repoz/Portfolio/src/components/DesignComponent.js",[],"/home/zoney/repoz/Portfolio/src/data/DesignData.js",[],{"ruleId":"91","replacedBy":"92"},{"ruleId":"93","replacedBy":"94"},{"ruleId":"95","severity":1,"message":"96","line":31,"column":7,"nodeType":"97","messageId":"98","endLine":31,"endColumn":13},{"ruleId":"95","severity":1,"message":"99","line":79,"column":7,"nodeType":"97","messageId":"98","endLine":79,"endColumn":13},{"ruleId":"95","severity":1,"message":"100","line":117,"column":9,"nodeType":"97","messageId":"98","endLine":117,"endColumn":20},{"ruleId":"95","severity":1,"message":"101","line":55,"column":10,"nodeType":"97","messageId":"98","endLine":55,"endColumn":17},"no-native-reassign",["102"],"no-negated-in-lhs",["103"],"no-unused-vars","'Rotate' is assigned a value but never used.","Identifier","unusedVar","'Center' is assigned a value but never used.","'handleClick' is assigned a value but never used.","'numbers' is assigned a value but never used.","no-global-assign","no-unsafe-negation"]